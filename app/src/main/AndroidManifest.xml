<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ifst">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/isftlogo"
        android:label="ISFT2020"
        android:roundIcon="@drawable/isft"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Event"></activity>
        <activity android:name=".Gallery" />
        <activity android:name=".Organizers" />
        <activity android:name=".Collaborators" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".splashScreen">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SpeakersList" />
    </application>

</manifest>